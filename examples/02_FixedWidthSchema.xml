<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://jsapar.tigris.org/JSaParSchema/2.0"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://jsapar.tigris.org/JSaParSchema/2.0 http://jsapar.tigris.org/JSaParSchema/2.1/JSaParSchema.xsd">
    <fixedwidthschema lineseparator="\n">
        <line linetype="Person" occurs="*">
            <cell name="First name" length="8"/>
            <cell name="Middle name" length="9" ignoreread="true"/>
            <cell name="Last name" length="9"/>
            <cell name="weight" length="5" padcharacter="0" alignment="right"><implieddecimal decimals="2"/></cell>
        </line>
    </fixedwidthschema>
</schema>
